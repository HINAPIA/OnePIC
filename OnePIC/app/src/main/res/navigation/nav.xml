<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/editFragment">

    <fragment
        android:id="@+id/editFragment"
        android:name="com.example.onepic.Edit.Fragment.EditFragment"
        android:label="EditFragment"
        tools:layout="@layout/fragment_edit">
        <action
            android:id="@+id/action_editFragment_to_rewindFragment"
            app:destination="@id/rewindFragment" />
        <action
            android:id="@+id/action_editFragment_to_magicPictureFragment"
            app:destination="@id/magicPictureFragment" />
    </fragment>
    <fragment
        android:id="@+id/rewindFragment"
        android:name="com.example.onepic.Edit.Fragment.RewindFragment"
        android:label="rewindFragment"
        tools:layout="@layout/fragment_rewind">
        <action
            android:id="@+id/action_rewindFragment_to_editFragment"
            app:destination="@id/editFragment" />
    </fragment>
    <fragment
        android:id="@+id/magicPictureFragment"
        android:name="com.example.onepic.Edit.Fragment.MagicPictureFragment"
        android:label="fragment_magic_picture"
        tools:layout="@layout/fragment_magic_picture" >
        <action
            android:id="@+id/action_magicPictureFragment_to_editFragment"
            app:destination="@id/editFragment" />
    </fragment>
</navigation>